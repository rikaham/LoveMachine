<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Ad0604</servlet-name>
		<servlet-class>servlet.Ad0604Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Ad0604</servlet-name>
		<url-pattern>/ad0604</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>mainservlet</servlet-name>
    	<servlet-class>servlet.MainServlet</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>mainservlet</servlet-name>
    	<url-pattern>/main</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>homereturnservlet</servlet-name>
    	<servlet-class>servlet.HomeReturnServlet</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>homereturnservlet</servlet-name>
    	<url-pattern>/homereturn</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>mutterservlet</servlet-name>
    	<servlet-class>database.Mutter</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>mutterservlet</servlet-name>
    	<url-pattern>/mutter</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
